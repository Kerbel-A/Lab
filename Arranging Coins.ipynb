   "source": [
    "class Solution:\n",
    "    def arrangeCoins(self, n: int) -> int:\n",
    "        k = 1\n",
    "        while True:\n",
    "            n -= k\n",
    "            if n < 0:\n",
    "                return k - 1\n",
    "            k += 1"
   ]
  }
