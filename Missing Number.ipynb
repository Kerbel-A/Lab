   "source": [
    "class Solution:\n",
    "    def missingNumber(self, nums: List[int]) -> int:\n",
    "        len_n = len(nums)\n",
    "        res = len_n\n",
    " \n",
    "        for i in range(len_n):\n",
    "            res ^= i ^ nums[i]\n",
    "        return res"
   ]
  }
